<view class="notelist">
  <block wx:for="{{imagePaths}}" wx:key="{{index}}">
    <view class="note-list">
      <view class="note-list-info" index="{{index}}">
        <image class="note-list-photo" bindtap='clickImg' src="{{item}}" id="{{index}}" />
      </view>
    </view>
  </block>
